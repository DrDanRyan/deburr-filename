{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [1, "single", "avoid-escape"],
    "no-use-before-define": 0,
    "new-cap": 0,
    "strict": 0,
    "no-process-exit": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "no-multiple-empty-lines": 0,
    "no-console": 0
  },
  "env": {
    "node": true
  }
}
